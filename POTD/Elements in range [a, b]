// Brute Approach
// Time: O(q x n)
// Space: O(1)
--------------------------------------------------------------------------------------
--------------------------------------------------------------------------------------
class Solution {
    public ArrayList<Integer> cntInRange(int[] arr, int[][] queries) {
        // code here
        ArrayList<Integer> res = new ArrayList<>();
        
        int n = arr.length;
        int q = queries.length;
        
        
        for(int i = 0; i < q; i++)
        {
            int count = 0;
            
            int start = queries[i][0];
            int end = queries[i][1];
            
            for(int j = 0; j < n; j++)
            {
                if(arr[j] >= start && arr[j] <= end)
                    count++;
            }
            
            res.add(count);
            
        }
        
        return res;
    }
}
--------------------------------------------------------------------------------------
--------------------------------------------------------------------------------------

// Optimal Approach
// Time: O(n logn + q logn)
// Space: O(1)
--------------------------------------------------------------------------------------
--------------------------------------------------------------------------------------
