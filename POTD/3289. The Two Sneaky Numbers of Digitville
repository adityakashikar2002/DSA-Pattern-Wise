// Time: O(n)
// Space: O(n)
--------------------------------------------------------------------------------------------------
--------------------------------------------------------------------------------------------------

class Solution {
    public int[] getSneakyNumbers(int[] nums) {
        Set<Integer> set = new HashSet<>();

        int[] res = new int[2];
        int k = 0;

        for(int num : nums)
        {
            if(set.contains(num))
            {
                res[k++] = num;
            }

            set.add(num);
        }

        return res;
    }
}
