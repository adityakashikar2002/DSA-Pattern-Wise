// Brute Approach
// Time: O(nÂ²)
// Space: O(1)
--------------------------------------------------------------------------------------------------------------------------
--------------------------------------------------------------------------------------------------------------------------
class Solution {
    public long maxSubarraySum(int[] nums, int k) {
        int n = nums.length;

        long maxSum = Long.MIN_VALUE;

        for(int i = 0; i < n; i++)
        {
            long sum = 0;

            for(int j = i; j < n; j++)
            {
                sum += nums[j];
                int len = j - i + 1;

                if(len % k == 0)
                {
                    maxSum = Math.max(maxSum, sum);
                }
            }
        }

        return maxSum;
    }
}

--------------------------------------------------------------------------------------------------------------------------
--------------------------------------------------------------------------------------------------------------------------
// Optimal Approach
// Time: O()
// Space: O()
--------------------------------------------------------------------------------------------------------------------------
