// Time: O(n logn)
// Space: O(logn)
----------------------------------------------------------------------------------------
class Solution {
    public int[] getNoZeroIntegers(int n) {
        int[] ans = new int[2];
        
        for(int i = 1; i < n; i++)
        {
            int j = n - i;

            if(!String.valueOf(i).contains("0") && !String.valueOf(j).contains("0"))
            {
                ans[0] = i;
                ans[1] = j;
                break;
            }  
        }

        return ans;
    }
}
