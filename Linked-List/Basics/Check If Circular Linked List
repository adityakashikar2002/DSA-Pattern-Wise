/* Structure of LinkedList
class Node
{
    int data;
    Node next;
    Node(int d)
    {
        data = d;
        next = null;
    }
}
*/
class Solution {
    boolean isCircular(Node head) {
        // Your code here
        Node temp = head;
        
        do
        {
            temp = temp.next;
        }
        while(temp != null && temp != head);
        
        if(temp == null)
            return false;
        
        return true;
    }
}
